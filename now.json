{
  "name": "chat",
  "version": 2,
  "builds": [
    {
      "src": "dist/*.html",
      "use": "@now/static"
    },
    {
      "src": "dist/*.js",
      "use": "@now/static"
    },
    {
      "src": "server.js",
      "use": "@now/node"
    }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "dist/index.html"
    }
  ]
}
